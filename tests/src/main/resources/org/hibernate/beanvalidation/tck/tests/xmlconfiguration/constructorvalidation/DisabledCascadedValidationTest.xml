<constraint-mappings
        xmlns="https://jakarta.ee/xml/ns/validation/mapping"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://jakarta.ee/xml/ns/validation/mapping validation-mapping-4.0.xsd" version="4.0">

    <bean class="org.hibernate.beanvalidation.tck.tests.xmlconfiguration.constructorvalidation.Cascaded">

        <constructor ignore-annotations="true">
            <parameter type="java.lang.String">
                <constraint annotation="jakarta.validation.constraints.NotNull"/>
            </parameter>
        </constructor>

    </bean>

</constraint-mappings>
