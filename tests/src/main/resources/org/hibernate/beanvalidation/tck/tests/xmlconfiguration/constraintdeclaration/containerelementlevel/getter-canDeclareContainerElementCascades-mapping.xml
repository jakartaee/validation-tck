<constraint-mappings
        xmlns="https://jakarta.ee/xml/ns/validation/mapping"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://jakarta.ee/xml/ns/validation/mapping
            https://jakarta.ee/xml/ns/validation/validation-mapping-4.0.xsd"
        version="4.0">

    <default-package>org.hibernate.beanvalidation.tck.tests.xmlconfiguration.constraintdeclaration.containerelementlevel</default-package>

    <bean class="org.hibernate.beanvalidation.tck.tests.xmlconfiguration.constraintdeclaration.containerelementlevel.ContainerElementTypeConstraintsForGetterXmlMappingTest$FishTank" ignore-annotations="false">
        <getter name="boss">
            <container-element-type>
                <valid />
            </container-element-type>
        </getter>
    </bean>

    <bean class="org.hibernate.beanvalidation.tck.tests.xmlconfiguration.constraintdeclaration.containerelementlevel.ContainerElementTypeConstraintsForGetterXmlMappingTest$Fish" ignore-annotations="false">
        <getter name="name">
            <constraint annotation="jakarta.validation.constraints.NotNull" />
        </getter>
    </bean>
</constraint-mappings>
